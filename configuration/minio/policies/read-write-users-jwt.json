{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Principal": {
          "AWS": ["*"]
        },
        "Action": ["s3:ListBucket"],
        "Resource": ["arn:aws:s3:::users"],
        "Condition": {
          "StringLike": { "s3:prefix": ["${jwt:preferred_username}/*"] }
        }
      },
      {
        "Effect": "Allow",
        "Principal": {
          "AWS": ["*"]
        },
        "Action": [
          "s3:AbortMultipartUpload",
          "s3:DeleteObject",
          "s3:GetObject",
          "s3:ListMultipartUploadParts",
          "s3:PutObject"
        ],
        "Resource": ["arn:aws:s3:::users/${jwt:preferred_username}/*"]
      }
    ]
  }
  